  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true,
    "transform": {
      "\\.(ts)$": "ts-jest"
    },
    "testEnvironment": "node",
    "testRunner": "jest-circus/runner",
    "testRegex": ".test.ts",
    "testPathIgnorePatterns": [
